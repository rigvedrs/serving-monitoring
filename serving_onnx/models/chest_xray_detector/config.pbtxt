name: "chest_xray_detector"
backend: "onnxruntime"
max_batch_size: 8
input [
  {
    name: "images"  # Input name from your YOLO model
    data_type: TYPE_FP32
    dims: [3, 640, 640]  # Standard YOLO input dimensions
  }
]
output [
  {
    name: "output0"  # Output name from your YOLO model
    data_type: TYPE_FP32
    dims: [84, 8400]  # YOLO output dimensions [84, 8400]
  }
]
instance_group [
  {
    count: 1
    kind: KIND_GPU
    gpus: [ 0 ]
  }
]
dynamic_batching {
  preferred_batch_size: [1, 4, 8]
  max_queue_delay_microseconds: 100000
}