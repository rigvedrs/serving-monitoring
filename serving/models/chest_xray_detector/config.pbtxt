name: "chest_xray_detector"
backend: "python"
max_batch_size: 16
input [
  {
    name: "INPUT_IMAGE"
    data_type: TYPE_STRING
    dims: [1]
  },
  {
    name: "CONFIDENCE_THRESHOLD"
    data_type: TYPE_FP32
    dims: [1]
  },
  {
    name: "IOU_THRESHOLD"
    data_type: TYPE_FP32
    dims: [1]
  }
]
output [
  {
    name: "DETECTIONS"
    data_type: TYPE_STRING
    dims: [-1]
  },
  {
    name: "INFERENCE_TIME"
    data_type: TYPE_FP32
    dims: [1]
  }
]
instance_group [
  {
    count: 1
    kind: KIND_GPU
    gpus: [ 0 ]
  }
]